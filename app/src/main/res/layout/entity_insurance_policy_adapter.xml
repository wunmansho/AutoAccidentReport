<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="5dp">


    <LinearLayout
        android:id="@+id/entity_insurance_policy_ll01"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:orientation="vertical"
        android:visibility="visible">

        <TextView
            android:id="@+id/tvIPO_ID"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:elevation="0dp"
            android:text="@string/todo"
            android:textColor="@android:color/black"
            android:textSize="12sp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/tvIPO_AID"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:elevation="0dp"
            android:text="@string/todo"
            android:textColor="@android:color/black"
            android:textSize="12sp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/tvIPO_HOLDER"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:elevation="0dp"
            android:text="@string/todo"
            android:textColor="@android:color/black"
            android:textSize="12sp"
            android:visibility="gone" />


        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:elevation="0dp"
            android:text="@string/policy_holder_heading"
            android:textColor="@android:color/black"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/tvPOLICY_HOLDER"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:elevation="0dp"
            android:text="@string/tv0001"
            android:textColor="@android:color/black"
            android:textSize="12sp" />


    </LinearLayout>

    <Space
        android:id="@+id/space01"
        android:layout_width="wrap_content"
        android:layout_height="10dp"

        android:layout_below="@+id/entity_insurance_policy_ll01" />

    <LinearLayout
        android:id="@+id/entity_insurance_policy_ll02"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/space01"
        android:layout_marginStart="5dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"

            android:layout_weight=".4"
            android:elevation="0dp"
            android:gravity="top"
            android:labelFor="@+id/tieXX_CNAM"
            android:text="@string/tv0014"
            android:textAlignment="gravity"
            android:textColor="@android:color/black"
            android:textSize="12sp" />


        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight=".6"

            android:elevation="0dp"
            android:gravity="top"
            android:labelFor="@+id/tieXX_PNUM"
            android:text="@string/tv0015"
            android:textAlignment="gravity"
            android:textColor="@android:color/black"
            android:textSize="12sp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/entity_insurance_policy_ll03"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/entity_insurance_policy_ll02"
        android:layout_marginStart="5dp"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/tvXX_CNAM"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_weight=".4"
            android:elevation="0dp"
            android:gravity="top"
            android:text="@string/todo"
            android:textAlignment="gravity"
            android:textColor="@android:color/black"
            android:textSize="12sp" />


        <TextView
            android:id="@+id/tvXX_PNUM"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight=".6"
            android:elevation="0dp"
            android:gravity="top"
            android:text="@string/todo"
            android:textAlignment="gravity"
            android:textColor="@android:color/black"
            android:textSize="12sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/entity_insurance_policy_ll04"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/entity_insurance_policy_ll03"
        android:layout_marginBottom="5dp"
        android:layout_marginStart="5dp"
        android:orientation="vertical"
        android:visibility="gone">


        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:elevation="0dp"
            android:text="@string/insured"
            android:textColor="@android:color/black"
            android:textSize="12sp" />


        <TextView
            android:id="@+id/tvXX_GENERIC_VALUE01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:elevation="0dp"
            android:text="@string/todo"
            android:textColor="@android:color/black"
            android:textSize="12sp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:elevation="0dp"
            android:text="@string/todo"
            android:textColor="@android:color/black"
            android:textSize="12sp"
            android:visibility="gone" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:elevation="0dp"
            android:text="@string/todo"
            android:textColor="@android:color/black"
            android:textSize="12sp"
            android:visibility="gone" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/entity_insurance_policy_ll04"
        android:layout_marginStart="5dp"
        android:orientation="vertical"
        android:visibility="gone">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="@string/todo"
            android:textColor="@android:color/black"
            android:textSize="12sp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="@string/todo"
            android:textColor="@android:color/black"
            android:textSize="12sp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="@string/todo"
            android:textColor="@android:color/black"
            android:textSize="12sp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="@string/todo"
            android:textColor="@android:color/black"
            android:textSize="12sp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="@string/todo"
            android:textColor="@android:color/black"
            android:textSize="12sp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="@string/todo"
            android:textColor="@android:color/black"
            android:textSize="12sp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="@string/todo"
            android:textColor="@android:color/black"
            android:textSize="12sp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="@string/todo"
            android:textColor="@android:color/black"
            android:textSize="12sp" />
    </LinearLayout>


</RelativeLayout>